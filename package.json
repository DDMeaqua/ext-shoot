{
  "name": "extension-shoot",
  "displayName": "Ext Shoot",
  "version": "0.0.1",
  "description": "A blazingly fast extension launcher for browser",
  "author": "River.<laolei@forkway.cn>",
  "contributors": [
    "River",
    "WtecHtec"
  ],
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.6.2",
    "@plasmohq/storage": "^1.9.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@stitches/react": "^1.2.8",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clipboard-polyfill": "^4.0.2",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "framer-motion": "^11.0.8",
    "ldrs": "^1.0.1",
    "lucide-react": "^0.358.0",
    "plasmo": "^0.74.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sonner": "^1.4.3",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "3.4.1"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@radix-ui/colors": "2.1.0",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "typescript": "5.3.3",
    "webextension-polyfill": "^0.10.0",
    "windy-radix-palette": "0.6.1"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "tabs",
      "management",
      "unlimitedStorage"
    ],
    "web_accessible_resources": [
      {
        "resources": [
          "tabs/icon-render.html"
        ],
        "matches": [
          "<all_urls>"
        ]
      }
    ]
  }
}
